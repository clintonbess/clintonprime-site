{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "composite": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "rootDir": "src",
    "outDir": "dist",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": false
  },
  "include": [
    "src"
  ]
}