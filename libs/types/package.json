{
  "name": "@clintonprime/types",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist tsconfig.build.tsbuildinfo && pnpm -w exec tsc -p libs/types/tsconfig.build.json",
    "clean": "rimraf dist",
    "rebuild": "pnpm clean && pnpm build"
  },
  "dependencies": {
    "react": "^19.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/react": "^19.1.16",
    "rimraf": "^6.0.1",
    "typescript": "^5.9.3"
  }
}